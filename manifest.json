{
    "name": "testExtention",
    "version": "0.1",
    "manifest_version": 2,
    "description": "This is a test Chrome Extention",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["background.js"]
    },
    
    "permissions": [
        "geolocation"
    ],

    "content_scripts": [
        {
          "matches": ["https://duckduckgo.com/*"],
          "js": ["script-duckduckgo.js"]
        }
      ]
}